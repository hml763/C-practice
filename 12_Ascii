//아스키 코드  
#include <stdio.h>
 
int main(void){
 
    int ASCII;
 
    for(ASCII = 0; ASCII <= 127; ASCII++){
 
        printf("%d\t%x\t%c\n", ASCII, ASCII, ASCII);
 
        if(ASCII % 20 == 0){
 
            printf("--------------------------\n");
            printf("10진수\t16진수\t아스키문자\n");
            printf("--------------------------\n");
 
        }
    }
 
    return 0;
}
